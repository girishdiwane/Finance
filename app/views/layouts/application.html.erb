<!DOCTYPE html>
<html>
<head>
  <title>Finance</title>
  <%= stylesheet_link_tag    "application","validationEngine.jquery","template", :media => "all" %>
  <%= javascript_include_tag "application", "jquery-1.8.2.min", "jquery.validationEngine-en", "jquery.validationEngine" %>
  <%= csrf_meta_tags %>
</head>
<body>
 <% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :id => "flash_#{name}" %>
<% end %>
<%= yield %>

</body>
</html>
